<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>MainWindow</servlet-name>
        <servlet-class>web.MainWindow</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsertEMP</servlet-name>
        <servlet-class>web.InsertEMP</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ShowAllEMP</servlet-name>
        <servlet-class>web.ShowAllEMP</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RemoveServlet</servlet-name>
        <servlet-class>web.RemoveServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ShowEMP</servlet-name>
        <servlet-class>web.ShowEMP</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ShowLogs</servlet-name>
        <servlet-class>web.ShowLogs</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AjaxResponse</servlet-name>
        <servlet-class>web.AjaxResponse</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MainWindow</servlet-name>
        <url-pattern>/MainWindow</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertEMP</servlet-name>
        <url-pattern>/InsertEMP</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ShowAllEMP</servlet-name>
        <url-pattern>/ShowAllEMP</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RemoveServlet</servlet-name>
        <url-pattern>/RemoveServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ShowEMP</servlet-name>
        <url-pattern>/ShowEMP</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ShowLogs</servlet-name>
        <url-pattern>/ShowLogs</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AjaxResponse</servlet-name>
        <url-pattern>/AjaxResponse</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
